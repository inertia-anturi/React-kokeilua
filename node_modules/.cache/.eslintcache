         <ul>\n                {opiskelijat.map((op) => <Rivi etunimi={op.etunimi} sukunimi={op.sukunimi} aloitusvuosi={op.aloitusvuosi} />)}\n            </ul>\n        </div>\n    );\n\n}\n\nconst Rivi = (props) => {\n    return (\n        <div>\n            <li>{props.etunimi}, {props.sukunimi}, {props.aloitusvuosi}</li>\n        </div>\n    );\n}\n\n\n\n\n\n\n\n\nfunction Teht6() {\n\n\n    const [visible, setVisible] = useState(true);\n\n\n\n    const data = [\n        { nimi: 'Mimmi', osoite: 'Mielitie 3', aloitusvuosi: '2019' },\n        { nimi: 'Veera', osoite: 'Virmatie 7', aloitusvuosi: '2020' },\n        { nimi: 'Verneri', osoite: 'Virtakuja 8', aloitusvuosi: '2021' },\n        { nimi: 'Lilli', osoite: 'Puttipolku 9', aloitusvuosi: '2022' }\n    ];\n    const otsikot = [\n        { nimi: 'NIMI', osoite: 'OSOITE', aloitusvuosi: 'ALOITUSVUOSI' }\n    ];\n    const otsikotENG = [\n        { nimi: 'NAME', osoite: 'ADDRESS', aloitusvuosi: 'START' }\n    ];\n\n\n\n    return (\n        <div>\n            <h1 style={{ color: 'green' }}>Taulukko</h1>\n            <button onClick={() => setVisible(!visible)}>{visible ? 'Piilota' : 'Näytä'}</button>\n            {visible && <div><Taulukko otsikot={otsikot} data={data} />\n                <Taulukko otsikot={otsikotENG} data={data} /></div>}\n\n        </div>\n    )\n}\n\nfunction Taulukko(props) {\n    console.log(\"konsoliin propsit:\", props);\n    let o = props.otsikot || [];\n    let d = props.data || [];\n\n    return (\n        <div>\n            \n            <table>\n                <Otsikko otsikot={o} />\n                <TauluRivi data={d} />\n            </table>\n        </div>\n    );\n\n}\nconst Otsikko = (props) => {\n    console.log(\"otsikkoprops:\", props)\n    const otsikot = props.otsikot.map((ots, index) => {\n        return (\n\n\n            <tr key={index}>\n                <th style={{ color: 'orange' }}>\n                    {ots.nimi}\n                </th>\n                <th style={{ color: 'red' }}>\n                    {ots.osoite}\n                </th>\n                <th style={{ color: 'brown' }}>\n                    {ots.aloitusvuosi}\n                </th>\n            </tr>\n\n        )\n    }\n    )\n    return <thead>{otsikot}</thead>\n}\n\nconst TauluRivi = (props) => {\n    console.log(\"tauluriviprops:\", props)\n    const rivit = props.data.map((riv, index) => {\n        return (\n            <tr key={index}>\n                <td>{riv.nimi}</td>\n                <td>{riv.osoite}</td>\n                <td>{riv.aloitusvuosi}</td>\n            </tr>\n        )\n    })\n\n    return <tbody>{rivit}</tbody>\n\n}\n\nexport { Lista, Rivi, Teht6, Taulukko, Otsikko, TauluRivi};","/Users/lauravihavainen/web-dev/React-kokeilua/React-kokeilua/src/reportWebVitals.js",[],["26","27"],{"ruleId":"28","severity":1,"message":"29","line":6,"column":18,"nodeType":"30","messageId":"31","endLine":6,"endColumn":22},{"ruleId":"28","severity":1,"message":"32","line":7,"column":17,"nodeType":"30","messageId":"31","endLine":7,"endColumn":21},{"ruleId":"28","severity":1,"message":"33","line":7,"column":40,"nodeType":"30","messageId":"31","endLine":7,"endColumn":47},{"ruleId":"28","severity":1,"message":"34","line":7,"column":49,"nodeType":"30","messageId":"31","endLine":7,"endColumn":58},{"ruleId":"28","severity":1,"message":"35","line":1,"column":8,"nodeType":"30","messageId":"31","endLine":1,"endColumn":12},{"ruleId":"28","severity":1,"message":"35","line":1,"column":8,"nodeType":"30","messageId":"31","endLine":1,"endColumn":12},{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},"no-unused-vars","'Arvo' is defined but never used.","Identifier","unusedVar","'Rivi' is defined but never used.","'Otsikko' is defined but never used.","'TauluRivi' is defined but never used.","'logo' is defined but never used.","no-native-reassign",["40"],"no-negated-in-lhs",["41"],"no-global-assign","no-unsafe-negation"]